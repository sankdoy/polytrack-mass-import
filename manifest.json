{
  "manifest_version": 3,
  "name": "PolyTrack Mass Import - TURBO LOADER 9000",
  "version": "1.3.4",
  "description": "Mass import tracks into PolyTrack from text files. The most EXTREME track loader ever created.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://www.kodub.com/*",
    "https://app-polytrack.kodub.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.kodub.com/*", "https://app-polytrack.kodub.com/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["injector.js"],
      "matches": ["https://www.kodub.com/*", "https://app-polytrack.kodub.com/*"]
    }
  ]
}
